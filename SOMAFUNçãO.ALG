Algoritmo "somafunção"

var
   v1, v2, s: inteiro
funcao soma(x, y:inteiro) :inteiro
inicio
 retorne x+y
fimfuncao
   
inicio
      escreva("Digite o primeiro valor: ")
      leia(v1)
      escreva("Digite o segundo valor: ")
      leia(v2)
      
      S <- soma(v1, v2)
      
      escreva("A soma dos valores é: ",S)


Fimalgoritmo